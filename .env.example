# Copy to .env and set your values.
# For local dev, PostgreSQL at localhost:5432 with user/password postgres/postgres
# and database "client_focus" is used by default if DATABASE_URL is not set.

DATABASE_URL=postgresql://postgres:postgres@localhost:5432/client_focus
PORT=5000

# NextAuth
NEXTAUTH_SECRET=change-this-to-a-random-secret-string
NEXTAUTH_URL=http://localhost:3000

# Admin credentials
ADMIN_EMAIL=your-email
ADMIN_PASSWORD=your-password

# Resend (email)
RESEND_API_KEY=your_resend_api_key_here
RESEND_FROM_EMAIL=your-email
RESEND_FROM_NAME=your-name-here